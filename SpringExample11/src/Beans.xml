<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

   <!-- Definition for javaCollection -->
   <bean id="javaCollection" class="com.tutorialspoint.JavaCollection">

      <!-- results in a setAddressList(java.util.List) call -->
      <property name="addressList">
         <list>
            <value>China</value>
            <value>Taiwan</value>
            <value>Hongkong</value>
            <value>Macao</value>
         </list>
      </property>

      <!-- results in a setAddressSet(java.util.Set) call -->
      <property name="addressSet">
         <set>
            <value>China</value>
            <value>Taiwan</value>
            <value>Hongkong</value>
            <value>Macao</value>
            <value>USA</value>
            <value>USA</value>
        </set>
      </property>

      <!-- results in a setAddressMap(java.util.Map) call -->
      <property name="addressMap">
         <map>
            <entry key="1" value="China"/>
            <entry key="2" value="Taiwan"/>
            <entry key="3" value="Hongkong"/>
            <entry key="4" value="Macao"/>
            <entry key="5" value="USA"/>
            <entry key="6" value="USA"/>
         </map>
      </property>

      <!-- results in a setAddressProp(java.util.Properties) call -->
      <property name="addressProp">
         <props>
            <prop key="one">China</prop>
            <prop key="two">Taiwan</prop>
            <prop key="three">Hongkong</prop>
            <prop key="four">Macao</prop>
            <prop key="five">USA</prop>
            <prop key="six">USA</prop>
         </props>
      </property>

   </bean>

</beans>